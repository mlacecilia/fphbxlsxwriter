-depincpath=zlib:/boot/develop/headers/3rdparty
-depincpathlocal=zlib:${HB_ADDONS}/../src/3rd/zlib
-depincpathlocal=zlib:../zlib
-depincpathlocal=zlib:libxlsxwriter/third_party/zlib

